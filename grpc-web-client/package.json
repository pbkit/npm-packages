{
  "name": "@pbkit/grpc-web-client",
  "packageManager": "yarn@3.0.2",
  "version": "0.0.0",
  "description": "pbkit grpc web client",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "git@github.com:pbkit/node-grpc-web-client.git",
  "author": "JongChan Choi <jong@chan.moe>",
  "license": "(MIT OR Apache-2.0)",
  "scripts": {
    "clean": "yarn clean:pollapo && yarn clean:pb && yarn clean:tsc",
    "clean:pollapo": "rm -rf .pollapo",
    "clean:pb": "rm -rf gen",
    "clean:tsc": "rm -rf lib",
    "build": "yarn build:pollapo && yarn build:pb && yarn build:tsc",
    "build:pollapo": "pollapo install",
    "build:pb": "pb gen ts -o src/gen --proto-path .pollapo --remove-ts-file-extension-in-import-statement pbkit/interface-pingpong-server/pingpong.proto",
    "build:tsc": "tsc",
    "test": "yarn jest",
    "test:watch": "yarn test --watch"
  },
  "files": [
    "/lib/**/*"
  ],
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.14.1",
    "@improbable-eng/grpc-web-node-http-transport": "^0.14.1",
    "google-protobuf": "^3.18.0-rc.2",
    "pbkit": "^0.0.12"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.5",
    "@types/jest": "^26.0.19",
    "jest": "^27.2.0",
    "jest-pnp-resolver": "^1.2.2",
    "ts-jest": "^27.0.2",
    "typescript": "^4.4.3"
  }
}
